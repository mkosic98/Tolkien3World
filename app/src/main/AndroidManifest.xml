<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.tolkworld">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".OthersActivity"></activity>
        <activity android:name=".EntsActivity" />
        <activity android:name=".OrcActivity" />
        <activity android:name=".MenActivity" />
        <activity android:name=".HobbitActivity" />
        <activity android:name=".MaiarActivity" />
        <activity android:name=".DwarvesActivity" />
        <activity android:name=".ElvesActivity" />
        <activity android:name=".LanguagesActivity" />
        <activity android:name=".PlacesActivity" />
        <activity android:name=".CharactersInfoActivity" />
        <activity android:name=".CharactersActivity" />
        <activity android:name=".AboutActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>